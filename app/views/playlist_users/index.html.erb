<p id="notice"><%= notice %></p>

<h1>Listing Playlist Users</h1>

<table>
  <thead>
    <tr>
      <th>Playlist</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @playlist_users.each do |playlist_user| %>
      <tr>
        <td><%= playlist_user.playlist_id %></td>
        <td><%= playlist_user.user_id %></td>
        <td><%= link_to 'Show', playlist_user %></td>
        <td><%= link_to 'Edit', edit_playlist_user_path(playlist_user) %></td>
        <td><%= link_to 'Destroy', playlist_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Playlist user', new_playlist_user_path %>
